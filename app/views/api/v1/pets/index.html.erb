<p id="notice"><%= notice %></p>

<h1>Api V1 Pets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_pets.each do |api_v1_pet| %>
      <tr>
        <td><%= link_to 'Show', api_v1_pet %></td>
        <td><%= link_to 'Edit', edit_api_v1_pet_path(api_v1_pet) %></td>
        <td><%= link_to 'Destroy', api_v1_pet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Pet', new_api_v1_pet_path %>
